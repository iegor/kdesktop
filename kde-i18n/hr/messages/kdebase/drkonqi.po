msgid "&Bug report"
msgstr "Izvještaj o pogreški"

#: toplevel.cpp:60
msgid "&Debugger"
msgstr "&Pronalaženje pogrešaka"

#: toplevel.cpp:64
msgid "&General"
msgstr "&Općenito"

