#, c-format
msgid ""
"Created thumbnail for: \n"
"%1"
msgstr ""
"%1 \n"
"のサムネイルを作成しました"

#: imgalleryplugin.cpp:211
msgid ""
"Creating thumbnail for: \n"
"%1\n"
" failed"
msgstr ""
