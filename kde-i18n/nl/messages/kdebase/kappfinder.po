msgstr "Omschrijving"

#: toplevel.cpp:65
msgid "Command"
msgstr "Commando"

#: toplevel.cpp:76
msgid "Summary:"
msgstr "Samenvatting:"

#: toplevel.cpp:80
msgid "Scan"
msgstr "Zoeken"

#: toplevel.cpp:85
msgid "Unselect All"
msgstr "Alles Deselecteren"

#: toplevel.cpp:245
#, c-format
msgid ""
"_n: Summary: found %n application\n"
"Summary: found %n applications"
msgstr ""
"Samenvatting: %n toepassing gevonden\n"
