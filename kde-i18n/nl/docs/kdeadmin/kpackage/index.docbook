<sect2>
  <title
>Bestand zoeken</title>
  
  <screenshot>
    <screeninfo
>Bestand zoeken in &kpackage;</screeninfo>
    <mediaobject>
      <imageobject>
	<imagedata fileref="searchf.png" format="PNG"/> </imageobject>
