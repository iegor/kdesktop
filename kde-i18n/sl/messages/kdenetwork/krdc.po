#: main.cpp:90
msgid "TightVNC encoding"
msgstr "Kodiranje TightVNC"

#: main.cpp:92
msgid "ZLib encoding"
msgstr "Kodiranje ZLib"

#: main.cpp:136
msgid "The password file '%1' does not exist."
msgstr "Datoteka z geslom  »%1« ne obstaja."

