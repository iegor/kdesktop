#: kpercentage.cpp:71
msgid "Number of tasks:"
msgstr "Število nalog:"

#: kpercentage.cpp:72
msgid "Level:"
msgstr "Stopnja:"

#: kpercentage.cpp:73
msgid "Choose an exercise type:"
msgstr "Izberite vrsto vaje:"

#: kpercentage.cpp:83
msgid "x% &of ?? = y"
msgstr "x% &od ?? = y"

#: kpercentage.cpp:84
#, c-format
msgid "x% of &y = ??"
msgstr "x% od &y = ??"

#: kpercentage.cpp:85
#, c-format
msgid "??% o&f x = y"
msgstr "??% o&d x = y"

#: kpercentage.cpp:86
