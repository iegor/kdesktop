<?xml version="1.0"?>
<!DOCTYPE logokeywords>

<logokeywords lang="sl" ver="0.01">

<!-- scope -->
  <command name="begin">
    <keyword>[</keyword>
  </command>
  
  <command name="end">
    <keyword>]</keyword>
  </command>

<!-- execution controllers -->
  <command name="while">
    <keyword>dokler</keyword>
  </command>
  
  <command name="if">
    <keyword>če</keyword>
  </command>
  
  <command name="else">
