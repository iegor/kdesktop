<?xml version="1.0"?>
<!DOCTYPE logokeywords>

<logokeywords lang="mk_MK" ver="0.01">

<!-- scope -->
  <command name="begin">
    <keyword>[</keyword>
  </command>

  <command name="end">
    <keyword>]</keyword>
  </command>



<!--
