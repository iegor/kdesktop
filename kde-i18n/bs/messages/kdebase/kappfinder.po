msgstr "Opis"

#: toplevel.cpp:65
msgid "Command"
msgstr "Naredba"

#: toplevel.cpp:76
msgid "Summary:"
msgstr "Sažetak:"

#: toplevel.cpp:80
msgid "Scan"
msgstr "Traži"

#: toplevel.cpp:85
msgid "Unselect All"
msgstr "Isključi sve"

