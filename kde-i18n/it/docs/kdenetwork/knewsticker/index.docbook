		<screen width="40"
><prompt
>%</prompt
> <userinput
><command
>dcop</command
> <option
>knewsticker</option
> <option
>KNewsTicker</option
> <option
>showIcons</option
></userinput>
                        true
                        <prompt
>%</prompt
>
                </screen>
	</listitem>
</varlistentry>
<varlistentry>
	<term
