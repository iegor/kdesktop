		<screen width="40"
><prompt
>%</prompt
> <userinput
><command
>dcop</command
> <option
>knewsticker</option
> <option
>KNewsTicker</option
> <option
>offlineMode</option
></userinput>
                        false
                        <prompt
>%</prompt
>
                </screen>
	</listitem>
</varlistentry>
<varlistentry>
