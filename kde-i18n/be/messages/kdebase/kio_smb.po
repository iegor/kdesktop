msgstr "<qt>Увядзіце спраўджвальныя звесткі для <b>%1</b></qt>"

#: kio_smb_auth.cpp:135
msgid ""
"Please enter authentication information for:\n"
"Server = %1\n"
"Share = %2"
msgstr ""
"Калі ласка, увядзіце спраўджвальныя звесткі для:\n"
"Сервер = %1\n"
"Рэсурс = %2"

#: kio_smb_auth.cpp:175
