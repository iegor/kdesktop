    <listitem>
      <para>
        <action
>Käivitab valitud teksti otsingu kasutaja määratud failis. </action>
      </para>
    </listitem>
  </varlistentry>
  <varlistentry>
    <term>
      <menuchoice
><guimenu
>Sõnaraamatud</guimenu
> <guimenuitem
>Otsi valitud teksti</guimenuitem
> <guimenuitem
>PO kompendium</guimenuitem
> </menuchoice>
