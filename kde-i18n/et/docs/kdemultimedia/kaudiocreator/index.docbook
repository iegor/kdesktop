     </term>
     <listitem>
      <para>
       <action
>Seadistused -> KAudioCreatori seadistamine -> CDDB -> Otsimine</action>
      </para>
      <para>
        <screenshot>
	       <mediaobject>
	           <imageobject>
	               <imagedata fileref="cddbconfigurationlookup.png" format="PNG"/>
	           </imageobject>
	           <textobject>
