msgid "<li>Protocol: %1</li>"
msgstr "<li>Protokoll: %1</li>"

#: kio/global.cpp:492
msgid "<li>Date and time: %1</li>"
msgstr "<li>Kuupäev ja kellaaeg: %1</li>"

#: kio/global.cpp:493
msgid "<li>Additional information: %1</li></ul>"
msgstr "<li>Lisainfo: %1</li></ul>"

