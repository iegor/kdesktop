msgstr "Χρήση &KIO"

#: javaopts.cpp:93
msgid "Shu&tdown applet server when inactive"
msgstr "&Τερματισμός εξυπηρετητή μικροεφαρμογών όταν είναι ανενεργός"

#: javaopts.cpp:101
msgid "App&let server timeout:"
msgstr "Χρονικό όριο εξυπηρετητή μικρο&εφαρμογών:"

#: javaopts.cpp:102
