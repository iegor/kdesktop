#: lskatview.cpp:428
msgid "      Game is drawn"
msgstr "תיקו"

#: lskatview.cpp:432
msgid "Player 1 - %1 won "
msgstr "שחקן 1 - %1 ניצח"

#: lskatview.cpp:436
msgid "Player 2 - %1 won "
msgstr "שחקן 2 - %1 ניצח"

