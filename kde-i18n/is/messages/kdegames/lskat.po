msgid "Game over"
msgstr "Leik lokið"

#: lskatview.cpp:417
msgid "Game was aborted - no winner"
msgstr "Leiknum var aflýst - enginn vann"

#: lskatview.cpp:428
msgid "      Game is drawn"
msgstr "        Jafntefli!"

#: lskatview.cpp:432
msgid "Player 1 - %1 won "
msgstr "Leikmaður 1 - %1 vann"

#: lskatview.cpp:436
msgid "Player 2 - %1 won "
msgstr "Leikmaður 2 - %1 vann"

