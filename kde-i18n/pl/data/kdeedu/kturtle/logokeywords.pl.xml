<?xml version="1.0"?>
<!DOCTYPE logokeywords>

<logokeywords lang="pl" ver="0.01">

<!-- scope -->
  <command name="begin">
    <keyword>[</keyword>
  </command>
  
  <command name="end">
    <keyword>]</keyword>
  </command>



<!--
