# Fargedemonstrasjon.

arkfarge 255,255,255
skjul

gjenta 5 [

for c = 0 til 255 [
  arkfarge 255-c,255,255
]
for c = 0 til 255 [
  arkfarge 0,255-c,255
]
for c = 0 til 255 [
  arkfarge c,0,255
]
for c = 0 til 255 [
  arkfarge 255,c,255-c
]
for c = 0 til 255 [
  arkfarge 255,255-c,0
]
for c = 0 til 255 [
  arkfarge 255,0,c
]
for c = 0 til 255 [
  arkfarge 255-c,0,255-c
]
for c = 0 til 255 [
  arkfarge c,c,c
]

]