> <option
>KNewsTicker</option
> <option
>slowedScrolling</option
></userinput>
                        false
                        <prompt
>%</prompt
>
                </screen>
	</listitem>
</varlistentry>
<varlistentry>
	<term
><command
>foregroundColor</command
></term>
	<listitem>
		<para
