msgid "Template"
msgstr "Skabelon"

#: filetemplates.cpp:1113
msgid "New..."
msgstr "Ny..."

#: filetemplates.cpp:1117
msgid "Edit..."
msgstr "Redigér..."

#: filetemplates.cpp:1125
msgid "Upload..."
msgstr "Overfør..."

#: filetemplates.cpp:1129
