msgstr "Genopret &forrige session"

#. i18n: file smserverconfigdlg.ui line 98
#: rc.cpp:31
#, no-c-format
msgid "Restore &manually saved session"
msgstr "Genopret &manuelt gemt session"

#. i18n: file smserverconfigdlg.ui line 106
#: rc.cpp:34
#, no-c-format
