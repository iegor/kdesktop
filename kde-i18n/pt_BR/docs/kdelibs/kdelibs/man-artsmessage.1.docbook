  <refnamediv>
    <refname
><command
>artsmessage</command
></refname>
    <refpurpose
>Um utilitário para mostrar as mensagens de erro do &arts;</refpurpose>
  </refnamediv>

  <refsynopsisdiv>
    <cmdsynopsis
><command
>artsmessage</command
> <group
><option
>Opções genéricas do &kde;</option
></group
