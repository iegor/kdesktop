msgid ""
"%1%\n"
"right"
msgstr ""
"%1%\n"
"ត្រឹមត្រូវ​"

#: kpercentmain.cpp:296 kpercentmain.cpp:308
msgid ""
"%1%\n"
"wrong"
msgstr ""
"%1%\n"
"ខុស​"

#: feedback_i18n.cpp:2
