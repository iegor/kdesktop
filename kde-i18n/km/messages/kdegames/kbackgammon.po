msgstr "ម៉ាស៊ីនបម្រើ"

#: engines/fibs/kbgfibs.cpp:296
msgid "Other"
msgstr "ផ្សេងៗ"

#: engines/fibs/kbgfibs.cpp:308
msgid "Server name:"
msgstr "ឈ្មោះ​ម៉ាស៊ីន​បម្រើ ៖"

#: engines/fibs/kbgfibs.cpp:309
msgid "Server port:"
msgstr "ច្រក​ម៉ាស៊ីន​បម្រើ ៖"

#: engines/fibs/kbgfibs.cpp:310
