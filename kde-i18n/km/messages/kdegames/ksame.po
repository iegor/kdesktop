msgstr "%1 ពណ៌"

#: KSameWidget.cpp:145
msgid "Do you want to resign?"
msgstr "តើ​អ្នក​ចង់​ឈប់​ឬ ?"

#: KSameWidget.cpp:146
msgid "Resign"
msgstr "ឈប់"

#: KSameWidget.cpp:155
msgid "Select Board"
msgstr "ជ្រើស​ក្ដារ"

#: KSameWidget.cpp:163
msgid "Select a board:"
msgstr "ជ្រើស​ក្ដារ​មួយ ៖"

