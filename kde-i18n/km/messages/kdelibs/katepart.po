msgid "Tabulators"
msgstr "ថេប"

#: part/katedialogs.cpp:475
msgid "&Insert spaces instead of tabulators"
msgstr "បញ្ចូល​ដកឃ្លា​ជំនួស​ឲ្យ​ថេប"

#: part/katedialogs.cpp:479
msgid "&Show tabulators"
msgstr "បង្ហាញ​ថេប"

#: part/katedialogs.cpp:485
