  <refnamediv>
    <refname
><command
>artsmessage</command
></refname>
    <refpurpose
>Utilitat per a mostrar els missatges d'error de &arts;</refpurpose>
  </refnamediv>

  <refsynopsisdiv>
    <cmdsynopsis
><command
>artsmessage</command
> <group
><option
>opcions genèriques de &kde;</option
></group
> <group
><option
>opcions genèriques de &Qt;</option
></group
> <group choice="opt"
> <option
>-e, --error</option
><option
>-w, --warning</option
><option
