"All\n"
"None\n"
"*.domain.com\n"
".domain.com\n"
"host.domain.com\n"
"nnn.*\n"
"nnn.nnn.*\n"
"nnn.nnn.nnn.*\n"
"nnn.nnn.nnn.nnn\n"
"nnn.nnn.nnn.nnn/mm\n"
