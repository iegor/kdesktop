msgid "Insert File Error"
msgstr "Meattáhus bidjadettiin fiilla sisa"

#: insertfileplugin.cpp:130
msgid ""
"<p>The file <strong>%1</strong> does not exist or is not readable, aborting."
msgstr ""
"<p><strong>%1</strong> fiila ii gávdno dahje ii leat logahahtti, "
"gaskkalduhttimin."

#: insertfileplugin.cpp:134
msgid "<p>Unable to open file <strong>%1</strong>, aborting."
msgstr "<p>Ii sáhttán rahpat <strong>%1</strong> fiilla, gaskkalduhttimin."

#: insertfileplugin.cpp:157
msgid "<p>File <strong>%1</strong> had no contents."
msgstr "<p><strong>%1</strong> fiillas ii lean sisdoallu."
