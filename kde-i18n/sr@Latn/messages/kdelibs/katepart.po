#: part/kateview.cpp:377
msgid "Hide Folding &Markers"
msgstr "Sakrij &markere za sažimanje"

#: part/kateview.cpp:380
msgid "Show &Icon Border"
msgstr "Prikaži &pojas za ikone"

#: part/kateview.cpp:384
