msgid "Exercise no. %1:"
msgstr "Vežba broj %1:"

#: kpercentmain.cpp:295 kpercentmain.cpp:307
msgid ""
"%1%\n"
"right"
msgstr ""
"%1%\n"
"tačno"

#: kpercentmain.cpp:296 kpercentmain.cpp:308
msgid ""
"%1%\n"
"wrong"
msgstr ""
"%1%\n"
"pogrešno"

