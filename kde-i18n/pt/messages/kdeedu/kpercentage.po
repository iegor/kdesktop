#: kpercentage.cpp:71
msgid "Number of tasks:"
msgstr "Número de tarefas:"

#: kpercentage.cpp:72
msgid "Level:"
msgstr "Nível:"

#: kpercentage.cpp:73
msgid "Choose an exercise type:"
msgstr "Escolha um tipo de exercício:"

#: kpercentage.cpp:83
msgid "x% &of ?? = y"
msgstr "x% &de ?? = y"

