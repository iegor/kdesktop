<?xml version="1.0"?>
<!DOCTYPE logokeywords>

<logokeywords lang="sr" ver="0.01">

<!-- scope -->
  <command name="begin">
    <keyword>[</keyword>
  </command>

  <command name="end">
    <keyword>]</keyword>
  </command>

<!-- execution controllers -->
  <command name="while">
    <keyword>док</keyword>
  </command>

  <command name="if">
    <keyword>ако</keyword>
  </command>

  <command name="else">
    <keyword>иначе</keyword>
  </command>

  <command name="for">
